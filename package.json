  "author": "KonstantinSerebryakov <KonstantinSerebryakov2000@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "npm run copyFlagIcons && quasar build",
    "copyFlagIcons": "node -e \"require('fs').cpSync('./node_modules/flag-icons/flags', './public/flag-icons', {recursive: true});\""
  },
