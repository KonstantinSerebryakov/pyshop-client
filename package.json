    "flag-icons": "^6.11.2",
    "inputmask": "^5.0.8",
    "libphonenumber-js": "^1.10.49",
    "mitt": "^3.0.1",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "uuid": "^9.0.1",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0",
    "vue-yandex-maps": "^0.11.17",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
